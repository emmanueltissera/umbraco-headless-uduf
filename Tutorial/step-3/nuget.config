<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <add key="umbracoMyGet" 
             value="https://www.myget.org/F/uaas/api/v3/index.json" 
             protocolVersion="3" />
        <!-- Added packages from a local folder as a fallback in case they are not present on umbraco MyGet feed in the future -->
        <add key="LocalPackages"
             value="LocalPackages" />
    </packageSources>
    <activePackageSource>
      <!-- this tells that all of them are active -->
      <add key="All" value="(Aggregate source)" />
    </activePackageSource>
</configuration>